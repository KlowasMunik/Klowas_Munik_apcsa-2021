public class AsciiBox
{
	public static void main(String[] args)
	{
		System.out.println("Munik Klowas \t  1/29/2021 \n\n" );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("+++++++++++++++++++++++++ " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );
		System.out.println("AAAAAAAAAAAAAAAAAAAAAAAAA " );


	}
}

