public class Variables
{
	public static void main ( String[] args )
	{
		//define 1 variable of each of the
		//following data types
		//byte		short		int 		long
		byte int = 2;
		//float		double
		float double = 4;
		//char      boolean		String
		char boolean string = true;
		//integer variables
		byte byteOne = 127;


		//decimal variables
		x = 1.5;

		//other integer types
		int x = 2;

		//other types


		//output your information here
		System.out.println("/////////////////////////////////");
		System.out.println("*                               *");
		System.out.println("*        integer types          *");
		System.out.println("*                               *");
		System.out.println("*8 bit - byteOne = "+byteOne+"\t\t*");


